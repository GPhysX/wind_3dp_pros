jj             =  44L       ;;  1999-11-05
jj             = 135L       ;;  2014-04-19
wind_h2_mfi_2_tplot,/LOAD_GSE,TRANGE=REFORM(all__trans[jj[0],*])
;;  Compute derivatives
get_data,1,DATA=temp,DLIM=dlim,LIM=lim
dydx           = REPLICATE(d,N_ELEMENTS(temp.X),4L)
d2ydx2         = REPLICATE(d,N_ELEMENTS(temp.X),4L)
wdth           = 512L
smbx           = SMOOTH(temp.Y[*,0],wdth[0],/NAN,/EDGE_TRUNCATE)
smby           = SMOOTH(temp.Y[*,1],wdth[0],/NAN,/EDGE_TRUNCATE)
smbz           = SMOOTH(temp.Y[*,2],wdth[0],/NAN,/EDGE_TRUNCATE)
smbm           = SMOOTH(temp.Y[*,3],wdth[0],/NAN,/EDGE_TRUNCATE)
smbf           = [[smbx],[smby],[smbz],[smbm]]
FOR k=0L, 3L DO dydx[*,k]   = DERIV(temp.X,temp.Y[*,k]);/smbf[*,3]
;FOR k=0L, 3L DO dydx[*,k]   = DERIV(temp.X,(temp.Y[*,k] - smbf[*,k]))
FOR k=0L, 3L DO d2ydx2[*,k] = DERIV(temp.X,dydx[*,k]);/ABS(dydx[*,k])
;;  Send to TPLOT
store_data,'dBdt',DATA={X:temp.X,Y:dydx,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
store_data,'d2Bdt2',DATA={X:temp.X,Y:d2ydx2,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
