tlimit,[zoom_st[jj,3],zoom_en[jj,3]]

;;  Use detrended values to determine amount of "spikiness"
wdth           = 3L
smbx           = SMOOTH(temp.Y[*,0],wdth[0],/NAN,/EDGE_TRUNCATE)
smby           = SMOOTH(temp.Y[*,1],wdth[0],/NAN,/EDGE_TRUNCATE)
smbz           = SMOOTH(temp.Y[*,2],wdth[0],/NAN,/EDGE_TRUNCATE)
smbm           = SMOOTH(temp.Y[*,3],wdth[0],/NAN,/EDGE_TRUNCATE)
smbf           = [[smbx],[smby],[smbz],[smbm]]
detbf          = temp.Y - smbf
detbfn         = detbf/(smbf[*,3] # REPLICATE(1d0,4L))
detbfn2        = detbf/ABS(smbf)
ddbdx          = REPLICATE(d,N_ELEMENTS(temp.X),4L)
d2dbdx2        = REPLICATE(d,N_ELEMENTS(temp.X),4L)
FOR k=0L, 3L DO ddbdx[*,k]   = DERIV(temp.X,detbf[*,k]);/smbf[*,3]
FOR k=0L, 3L DO d2dbdx2[*,k] = DERIV(temp.X,dydx[*,k]);/smbf[*,3]
;;  Send to TPLOT
store_data,'ddBdt',DATA={X:temp.X,Y:ddbdx,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
store_data,'d2dBdt2',DATA={X:temp.X,Y:d2dbdx2,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
store_data,'detrendB',DATA={X:temp.X,Y:detbf,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
store_data,'detrendB_Bo',DATA={X:temp.X,Y:detbfn,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
store_data,'detrendB_Bo2',DATA={X:temp.X,Y:detbfn2,TSHIFT:temp.TSHIFT[0]},DLIM=dlim,LIM=lim
options,['ddBdt','d2dBdt2','detrendB','detrendB_Bo','detrendB_Bo2'],'YTITLE'
